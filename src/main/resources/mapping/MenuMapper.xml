<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yzl.springbootsecurity.mapper.MenuMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yzl.springbootsecurity.entity.Menu">
        <id column="menu_id" property="menuId" />
        <result column="menu_url" property="menuUrl" />
        <result column="menu_name" property="menuName" />
        <result column="menu_icon" property="menuIcon" />
        <result column="menu_index" property="menuIndex" />
        <result column="menu_pid" property="menuPid" />
        <result column="menu_type" property="menuType" />
        <result column="menu_status" property="menuStatus" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        menu_id, menu_url, menu_name, menu_icon, menu_index, menu_pid, menu_type, menu_status
    </sql>

</mapper>
